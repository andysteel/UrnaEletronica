<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" 
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml"
	xmlns:ui="http://java.sun.com/jsf/facelets"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:p="http://primefaces.org/ui">

<ui:composition template="/WEB-INF/templates/template.xhtml">
<ui:define name="conteudo">
<h:body styleClass="corpo">
<br/>
	<p:panelGrid columns="3" styleClass="grid2">
		<p:outputLabel value="Bem vindo candidato: #{logadoBean.logado()}"/><a href="/Urna/candidato/j_spring_security_logout">logout</a>
		<a href="/Urna/index.xhtml">voltar</a>
	</p:panelGrid>
	<br/>
	<br/>
		<p:outputLabel value="ZAKK WYLDE POR UM MUNDO MELHOR !" styleClass="texto2" rendered="#{logadoBean.logado() eq 'ZAKK'?'true' :null}"/>
		<p:outputLabel value="SLASH POR UM MUNDO MELHOR !" styleClass="texto2" rendered="#{logadoBean.logado() eq 'SLASH'?'true' :null}"/>
		<p:outputLabel value="DARRELL POR UM MUNDO MELHOR !" styleClass="texto2" rendered="#{logadoBean.logado() eq 'DARRELL'?'true' :null}"/>
		<br/>
	
	<p:panelGrid columns="2" styleClass="grid3" rendered="#{logadoBean.logado() eq 'ZAKK'?'true' :null}">
		<p:graphicImage value="/resources/zakk_2.jpg" width="300" height="400"/>
		<p:barChart id="basic" value="#{graficoZakkBean.categoryModel}" legendPosition="ne" title="Acompanhe sua quantidade de votos" 
			min="0" max="100" animate="true" styleClass="grafico"/>
	</p:panelGrid>
	
	<p:panelGrid columns="2" styleClass="grid3" rendered="#{logadoBean.logado() eq 'SLASH'?'true' :null}">
		<p:graphicImage value="/resources/slash.jpg" width="300" height="400"/>
		<p:barChart id="basic" value="#{graficoSlashBean.categoryModel}" legendPosition="ne" title="Acompanhe sua quantidade de votos" 
			min="0" max="100" animate="true" styleClass="grafico"/>
	</p:panelGrid>
	
	<p:panelGrid columns="2" styleClass="grid3" rendered="#{logadoBean.logado() eq 'DARRELL'?'true' :null}">
		<p:graphicImage value="/resources/dime_1.jpg" width="300" height="400"/>
		<p:barChart id="basic" value="#{graficoDimeBean.categoryModel}" legendPosition="ne" title="Acompanhe sua quantidade de votos" 
			min="0" max="100" animate="true" styleClass="grafico"/>
	</p:panelGrid>

<br/>
<br/>
<br/>
</h:body>
</ui:define>
</ui:composition>
</html>